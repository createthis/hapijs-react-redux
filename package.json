{
  "name": "hapijs-react-redux",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions@0.0.10",
    "start": "node server",
    "dev": "NODE_ENV=development nodemon --config nodemon.json --exec ./node_modules/.bin/babel-node server",
    "devnomon": "NODE_ENV=development node ./node_modules/.bin/babel-node server",
    "serve": "node dist/server.js",
    "lint": "./node_modules/.bin/eslint --ext js --ext jsx",
    "test": "NODE_ENV=test node_modules/.bin/mocha",
    "reloadseed": "rm -f db.development.sqlite && NODE_ENV=development ./node_modules/.bin/sequelize db:migrate && NODE_ENV=development ./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:all --inspect",
    "coverage": "NODE_ENV=test node_modules/.bin/nyc --reporter=text --reporter=html node_modules/.bin/mocha",
    "clear-dist": "rm -Rf dist/* && rm -f dist/.env",
    "build-babel": "babel . --ignore .git,bin,dist,client,test,test_e2e,migrations,node_modules,webpack.config.js,seeders -d dist",
    "build-link-views": "./bin/link_views.sh && npm run build-copy-dot-env",
    "build-copy-dot-env": "cp .env dist/ || exit 0",
    "build-copy-public": "mkdir -p dist/public && ( cp -Rp public/* dist/public || exit 0 )",
    "build-webpack": "./node_modules/.bin/babel-node ./node_modules/.bin/webpack-cli build --output-path dist/",
    "build-link-node-modules": "ln -s ../node_modules dist/",
    "build": "npm run clear-dist && npm run build-babel && npm run build-link-views && npm run build-copy-public && npm run build-webpack && npm run build-link-node-modules",
    "build-for-development": "npm run build && mkdir -p dist/test/data && ( cp db.development.sqlite dist/ || exit 0 )",
    "migrate-dev": "NODE_ENV=development ./node_modules/.bin/sequelize db:migrate",
    "postinstall": "bin/setup_git_hooks.sh"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "all": true,
    "require": [
      "@babel/register"
    ],
    "extension": [
      ".jsx"
    ],
    "exclude": [
      "**/migrations/**",
      "**/seeders/**",
      "**/dist/**",
      "**/bin/**",
      "**/config/**",
      "**/webpack.config.js",
      "**/models/index.js",
      "**/abilities/**",
      "**/server.js",
      "**/*.spec.js",
      "**/build/**",
      "**/*.spec.js",
      "**/test_helpers/**",
      "**/test/**",
      "**/client/index.jsx",
      "**/client/containers/**",
      "**/coverage/**"
    ]
  },

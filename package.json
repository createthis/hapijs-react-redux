{
  "name": "hapijs-react-redux",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "dev": "NODE_ENV=development nodemon --config nodemon.json --exec ./node_modules/.bin/babel-node server",
    "devnomon": "NODE_ENV=development node ./node_modules/.bin/babel-node server",
    "serve": "node dist/server.js",
    "lint": "eslint --ext js --ext jsx components || exit 0",
    "test": "NODE_ENV=test node_modules/.bin/mocha --exit --timeout 40000 --full-trace --require @babel/register",
    "reloadseed": "rm -f db.development.sqlite && NODE_ENV=development ./node_modules/.bin/sequelize db:migrate && NODE_ENV=development ./node_modules/.bin/babel-node ./node_modules/.bin/sequelize db:seed:all --inspect",
    "coverage": "NODE_ENV=test node_modules/.bin/nyc --reporter=text --reporter=html node_modules/.bin/mocha --full-trace --exit --timeout 40000 --require @babel/register",
    "build": "babel . --ignore dist,client,test,migrations,node_modules,webpack.config.js,seeders -d dist && ./bin/link_views.sh"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "all": true,
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ],
    "extension": [
      ".jsx"
    ],
    "exclude": [
      "**/migrations/**",
      "**/seeders/**",
      "**/dist/**",
      "**/bin/**",
      "**/config/**",
      "**/webpack.config.js",
      "**/models/index.js",
      "**/abilities/**",
      "**/server.js",
      "**/*.spec.js",
      "**/build/**",
      "**/*.spec.js",
      "**/test_helpers/**",
      "**/test/**",
      "**/client/index.jsx",
      "**/client/containers/**",
      "**/coverage/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/createthis/hapijs-react-redux.git"
  },
  "author": "Jesse CreateThis <jesse@createthis.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/createthis/hapijs-react-redux/issues"
  },
  "homepage": "https://github.com/createthis/hapijs-react-redux#readme",
